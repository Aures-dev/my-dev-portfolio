{
    "version": 2,
    "builds": [
        {
            "src": "/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/public/$1",
            "headers": {
                "Cache-Control": "max-age=31536000"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}